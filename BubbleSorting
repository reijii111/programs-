#include <iostream>
int* BubbleSorting(int* array, int size)
{
for( int i = 1; i < size; ++i){
for( int r = 0; r < size-i; r++){
if(array[r] > array[r+1]){
int value = array[r];
array[r] = array[r+1];
array[r+1] = value;
}
}
}
return array;
}
int main() {
int input[] = {-25, -26, 36, 69, 82, -19, 89, 88, -4, -2};
int *output = BubbleSorting(input, 10);
for (int i = 0; i < 10; ++i)
std::cout Â« output[i] Â« " ";
return 0;
}
